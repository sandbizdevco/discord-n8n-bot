# Discord Bot Token (from Discord Developer Portal)
DISCORD_TOKEN=your_bot_token_here

# n8n Webhook URL (your n8n instance webhook endpoint)
N8N_WEBHOOK_URL=https://your-n8n-instance.railway.app/webhook/discord

# Optional: Comma-separated list of allowed channel IDs
# Leave empty to allow all channels
ALLOWED_CHANNEL_IDS=

# Optional: Bot command prefix (default is !)
BOT_PREFIX=!

# Optional: Show error messages to users (true/false)
SHOW_ERRORS=false
